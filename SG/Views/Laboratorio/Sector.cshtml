@model Entities.Laboratorio

@{
    ViewBag.Title = "Sector";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Datatable -->
<link href="~/Content/vendor/datatables/css/jquery.dataTables.min.css" rel="stylesheet">
<!-- Custom Stylesheet -->
<div class="content-body">
    <div class="container-fluid">

        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title">Sectores del laboratario @Model.nombre</h4>
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table id="example3" class="display" style="min-width: 845px">
                            <thead>
                                <tr>

                                    <th>Sector</th>
                                    <th>Estacion</th>
                                   
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model.listaSector)
                                {


                                    <tr>

                                        <td>@item.sector</td>
                                        <td>@item.estacion</td>

                                    </tr>

                                }

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>



    </div>
</div>
