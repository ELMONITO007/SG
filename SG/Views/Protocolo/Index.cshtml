@model Entities.Protocolo

@{
    ViewBag.Title = "Index";
    Layout = null;
}
<link href="~/Content/vendor/jquery-nice-select/css/nice-select.css" rel="stylesheet" />
<link href="~/Content/css/Print.css" rel="stylesheet" />
<link href="~/Content/vendor/owl-carousel/owl.carousel.css" rel="stylesheet" />
<!-- Style css -->

<link href="~/Content/css/style.css" rel="stylesheet" />

<!-- row -->
<div class="card" style="height:1754px;width:1220px;background-color:dodgerblue">

    <div class="card-header">
        <div class="d-flex align-items-center mb-4 flex-wrap">

            <h4 style="text-align:center; margin-left:100px" class="fs-30 font-w600  me-auto align-content-center">Protocolo de Ensayos N° @Model.codigo</h4>

        </div>

        <div class="d-flex align-items-end mb-4 flex-wrap">

          
                <img src="~/img/Sudamerican Green_free-file.png" style="align-items:end" />
               
        </div>
    </div>
    <div class="container-fluid">

        <div class="row">
            <div class="col-xl-3 col-xxl-4">
                <div class="row">
                    <div class="col-xl-12">
                        <div class=" card">

                            <div class="card">
                                <div class="card-header border-0 pb-0">
                                    <h4 class="fs-20 mb-0">Datos</h4>
                                </div>
                                <div class="card-body pt-4">
                                    <div class="mb-3 d-flex">
                                        <h5 class="mb-1 fs-14 custom-label">Codigo:</h5>
                                        <span>@Model.herramienta.codigoViejo</span>
                                    </div>
                                    <div class="mb-3 d-flex">
                                        <h5 class="mb-1 fs-14 custom-label">Fecha:</h5>
                                        <span>@Model.fechaEnsayo</span>
                                    </div>
                                    <div class="mb-3 d-flex">
                                        <h5 class="mb-1 fs-14 custom-label">Vencimiento:</h5>
                                        <span>@Model.herramienta.proximaActualizacion</span>
                                    </div>
                                    <div class="mb-3 d-flex">
                                        <h5 class="mb-1 fs-14 custom-label">Elemento:</h5>
                                        <span>@Model.herramienta.tipoHerramienta.tipoHerramienta</span>
                                    </div>
                                    <div class="mb-3 d-flex">
                                        <h5 class="mb-1 fs-14 custom-label">Tipos: </h5>
                                        <span>@Model.herramienta.tipoHerramienta.tipoEnsayo</span>
                                    </div>
                                    <div class="mb-3 d-flex">
                                        <h5 class="mb-1 fs-14 custom-label">Norma:</h5>
                                        <span>@Model.herramienta.tipoHerramienta.norma.codigoNorma</span>
                                    </div>
                                    <div class="mb-3 d-flex">
                                        <h5 class="mb-1 fs-14 custom-label">Solicitado por:</h5>
                                        <span>@Model.herramienta.usuarios.Nombre @Model.herramienta.usuarios.Apellido</span>
                                    </div>

                                </div>

                            </div>
                            <div class="card">
                                <div class="card-header border-0 pb-0">
                                    <h4 class="fs-20 mb-0">Resultado</h4>
                                </div>
                                <div class="card-body pt-4">

                                    <div class="mb-3 d-flex">
                                        <h5 class="mb-1 fs-14 custom-label">Temp: @Model.temperatura°</h5>  <h5 class="mb-1 fs-14 custom-label">Humedad: @Model.hmedadad%</h5>

                                    </div>

                                    @foreach (var item in Model.protocoloItem)
                                    {

                                        <div class="mb-3 d-flex">
                                            <pre style="font-size:12px">@item.ItemNorma.nombre:  @item.valor</pre>
                                          
                                        </div>
                                      



                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-xl-7 col-xxl-8">
                <div class="row">
                    <div class="col-xl-12">
                        <div class="card">
                            <div class="card-header d-block">
                                <h4 class="fs-20 d-block">Instrumentos empleado</a></h4>

                            </div>
                            <div class="card-body">

                                <div class="ms-4">
                                    @foreach (var subItem in Model.instrumentos)
                                    {
                                        <div class="col-md-auto">
                                            <pre style="font-size:14px"> <b>Instrumento:</b> @subItem.tipoHerramienta.tipoHerramienta  <b>Marca:</b> @subItem.marca <b>Codigo:</b> @subItem.codigoViejo </pre>
                                            <h5></h5>


                                        </div>
                                    }
                                </div>


                            </div>
                            <br />
                            <hr />

                            <div style="break-after:page">
                                <div class="card-header d-block ">
                                    <h4 class="fs-20 d-block">Precedimiento empleado</h4>
                                    <br />
                                    <h4 class="fs-15 d-block">Croquis</h4>
                                    <img src="~/img/croquis perdiga.png" />

                                    <p>@Model.herramienta.tipoHerramienta.norma.protocolo </p>
                                    <hr />

                                </div>
                                <div class="card-header d-block">
                                    <h4 class="fs-20 d-block">Conlusion Final</h4>
                                    <br />


                                    <p> @Model.conclusion </p>
                                    <hr />

                                </div>


                            </div>
                        </div>
                    </div>
                </div>
            </div>
     
        </div>

        <div class="row">
           

        </div>
    </div>


 






    <div class="card-fotter" style="margin-left:10px">@Model.laboratorio.piePagina</div>
</div>


<script src="~/Content/vendor/global/global.min.js"></script>
<script src="~/Content/vendor/chart.js/Chart.bundle.min.js"></script>


<script src="~/Content/vendor/jquery-nice-select/js/jquery.nice-select.min.js"></script>
<!-- Apex Chart -->
<script src="~/Content/vendor/apexchart/apexchart.js"></script>



<script src="~/Content/vendor/chart.js/Chart.bundle.min.js"></script>
<!-- Chart piety plugin files -->

<script src="~/Content/vendor/peity/jquery.peity.min.js"></script>
<!-- Dashboard 1 -->

<script src="~/Content/js/dashboard/dashboard-1.js"></script>

<script src="~/Content/vendor/owl-carousel/owl.carousel.js"></script>

<script src="~/Content/js/custom.min.js"></script>

<script src="~/Content/js/dlabnav-init.js"></script>


<script>
    jQuery(document).ready(function () {
        setTimeout(function () {
            dlabSettingsOptions.version = 'light';
            new dlabSettings(dlabSettingsOptions);
        }, 1500)
    });
    function JobickCarousel() {

        /*  testimonial one function by = owl.carousel.js */
        jQuery('.front-view-slider').owlCarousel({
            loop: false,
            margin: 30,
            nav: true,
            autoplaySpeed: 3000,
            navSpeed: 3000,
            autoWidth: true,
            paginationSpeed: 3000,
            slideSpeed: 3000,
            smartSpeed: 3000,
            autoplay: false,
            animateOut: 'fadeOut',
            dots: true,
            navText: ['', ''],
            responsive: {
                0: {
                    items: 1
                },

                480: {
                    items: 1
                },

                767: {
                    items: 3
                },
                1750: {
                    items: 3
                }
            }
        })
    }

    jQuery(window).on('load', function () {
        setTimeout(function () {
            JobickCarousel();
        }, 1000);
    });
</script>